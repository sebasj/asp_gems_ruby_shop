<h1>New Product</h1>

<%= form_for :post, url: products_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :desc %><br>
    <%= f.text_area :desc %>
  </p>

  <p>
    <%= f.label :date_start %><br>
    <%= f.text_area :date_start %>
  </p>

  <p>
    <%= f.label :maker %><br>
    <%= f.text_area :maker %>
  </p>

  <p>
    <%= f.label :stock %><br>
    <%= f.text_area :stock %>
  </p>

 
  <p>
    <%= f.submit %>
  </p>
<% end %>
